{
  "name": "fleetform",
  "version": "1.0.0",
  "description": "A docker manager.",
  "main": "dist/index.js",
  "bin": {
    "fleetform": "bin"
  },
  "scripts": {
    "dev": "nodemon --ext '*.ts' -w ${PWD}/src -x 'npm run build; npm run start'",
    "start-watch": "nodemon -w ./resources -x 'npm run start'",
    "build-watch": "nodemon --ext '*.ts' -w ${PWD}/src -x 'npm run build'",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "tsnode": "ts-node src/index.ts"
  },
  "author": "HalsMaulMajo",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/node": "^16.10.2",
    "nodemon": "^2.0.13",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  }
}